		<script type="text/javascript">
			//读写属性节点
			window.onload = function(){
				//点击每个li节点，若li节点的文本值没有 ^^ 开头，加上，有则去除
				
				var str = "^^abc";
				var reg = "/^\^{2}/g";
				alert(reg.test(str));
				str = str.replace(reg)
				var str2 = "abc";
				alert(reg.test(str2));
			}
		
		</script>
		
